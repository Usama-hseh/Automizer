{
  "id": "automizer",
  "name": "Automizer",
  "version": "1.0.0",
  "main": "index.html",
  "host": [
    {
      "app": "PS",
      "minVersion": "22.0.0"
    }
  ],
  "manifestVersion": 5,
  "requiredPermissions": {
    "localFileSystem": "fullAccess",
    "allowCodeGenerationFromStrings": true,
    "network": {
      "domains": ["https://*.google.com"]
    },
    "launchProcess": {
      "schemes": ["http", "https"],
      "extensions": [".svg", ".png"]
    }
  },
  "entrypoints": [
    {
      "type": "panel",
      "id": "automizer",
      "minimumSize": {
        "width": 350,
        "height": 500
      },
      "maximumSize": {
        "width": 700,
        "height": 900
      },
      "preferredDockedSize": {
        "width": 350,
        "height": 900
      },
      "preferredFloatingSize": {
        "width": 350,
        "height": 900
      },
      "icons": [
        {
          "width": 23,
          "height": 23,
          "path": "icons/dark.png",
          "scale": [1, 2],
          "theme": ["darkest", "dark", "medium"]
        },
        {
          "width": 23,
          "height": 23,
          "path": "icons/light.png",
          "scale": [1, 2],
          "theme": ["lightest", "light"]
        }
      ],
      "label": {
        "default": "Automizer"
      }
    }
  ],
  "icons": [
    {
      "width": 23,
      "height": 23,
      "path": "icons/dark.png",
      "scale": [1, 2],
      "theme": ["darkest", "dark", "medium"]
    },
    {
      "width": 23,
      "height": 23,
      "path": "icons/light.png",
      "scale": [1, 2],
      "theme": ["lightest", "light"]
    }
  ]
}
