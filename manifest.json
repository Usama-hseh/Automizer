{
	"id": "Automizer_V1",
	"name": "Automizer",
	"version": "1.0.1",
	"main": "index.html",
	"host": [
	  {
		"app": "PS",
		"minVersion": "22.0.0"
	  }
	],
	"manifestVersion": 5,
	"requiredPermissions": {
	  "allowCodeGenerationFromStrings": true,
	  "network": {
		"domains": [
		  "https://code.jquery.com/",
		  "https://cdn.jsdelivr.net/",
		  "https://mockible.com/",
		  "https://cdnjs.cloudflare.com"
		]
	  }
	},
	"entrypoints": [
	  {
		"type": "panel",
		"id": "vanilla",
		"minimumSize": {
		  "width": 350,
		  "height": 500
		},
		"maximumSize": {
		  "width": 700,
		  "height": 900
		},
		"preferredDockedSize": {
		  "width": 350,
		  "height": 900
		},
		"preferredFloatingSize": {
		  "width": 350,
		  "height": 900
		},
		"icons": [
		  {
			"width": 23,
			"height": 23,
			"path": "icons/icon-blue.png",
			"scale": [1, 2],
			"theme": ["darkest", "dark", "medium"]
		  },
		  {
			"width": 23,
			"height": 23,
			"path": "icons/icon-blue.png",
			"scale": [1, 2],
			"theme": ["lightest", "light"]
		  }
		],
		"label": {
		  "default": "Automizer"
		}
	  }
	],
	"icons": [
	  {
		"width": 23,
		"height": 23,
		"path": "icons/icon-blue.png",
		"scale": [1, 2],
		"theme": ["darkest", "dark", "medium"]
	  },
	  {
		"width": 23,
		"height": 23,
		"path": "icons/icon-blue.png",
		"scale": [1, 2],
		"theme": ["lightest", "light"]
	  }
	]
  }
  